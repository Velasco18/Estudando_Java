package br.com.fabricadeprogramador.aula8;

public class ProgramaPrincipalMatrizes {

	public static void main(String[] args) {
		
		//Estanciando a classe Aluno
		Aluno aluno = new Aluno();
		aluno.limparNotas();
		
		aluno.adicioarNotas(10); //[0,0] - posição 1
		aluno.adicioarNotas(8.5); //[0,1] - posição 2
		aluno.adicioarNotas(9.7); //[1,0] - posição 3
		aluno.adicioarNotas(6.5); //[1,1] - posição 4
		
		double notaBuscada = aluno.buscarNota(6);
		System.out.println(notaBuscada);

	}

}
